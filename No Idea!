# Enter your code here. Read input from STDIN. Print output to STDOUT
n,m=map(int,input().split())
arr=list(map(int,input().split()))
s1=set(map(int,input().split()))
s2=set(map(int,input().split()))
happiness=0
for e in arr:
    if e in s1:
        happiness+=1
    elif e in s2:
        happiness-=1
    else: None
print(happiness)
